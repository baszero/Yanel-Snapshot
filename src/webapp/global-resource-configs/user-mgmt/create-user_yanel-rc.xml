  <yanel:property name="xslt" value="/xslt/usecase.xsl"/>
  
  <yanel:custom-config>
    <views>
      <view id="default" type="jelly">
        <template>rthtdocs:/create-user.jelly</template>
      </view>
      <view id="done" type="jelly">
