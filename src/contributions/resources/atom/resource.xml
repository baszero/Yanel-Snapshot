<?xml version="1.0"?>

<resource xmlns="http://www.wyona.org/yanel/1.0"
  name="atom"
  namespace="http://www.wyona.org/yanel/resource/1.0"
  class="org.wyona.yanel.impl.resources.AtomFeedResource"
  >
<description>
Atom Feed Generator
</description>

<rtd>
  <!-- Alternative XSLT (otherwise xslt/atomfeed2xhtml.xsl will be used) -->
  <property name="xslt"/>
  <property name="mime-type"/>
  <property name="feed-title"/>
  <!-- Yarep path of entries -->
  <property name="entries-path"/>
  <!-- URL of this feed (proxy ...) -->
  <property name="self-url"/>
  <!-- Base URL of entries (proxy ..) -->
  <property name="entries-url"/>
  <property name="author"/>
</rtd>
</resource>
